{% if page_number > 1 %}<a href="{{ url }}/{{ page_number - 1 }}">Prev</a>{% else %} Prev {% endif %}
{% for page in range (1, page_count) %}
    <a href="{{ url }}/{{ page }}">{{ page }}</a>
{% endfor %}
{% if page_number < page_count %}<a href="{{ url }}/{{ page_number + 1 }}">Next</a>{% else %} Next {% endif %}